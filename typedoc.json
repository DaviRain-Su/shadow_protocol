{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "packages/core/src/index.ts",
    "packages/client/src/index.ts",
    "packages/server/src/index.ts",
    "packages/solana/src/index.ts",
    "packages/relayer/src/index.ts"
  ],
  "out": "docs/api",
  "name": "Px402 API Documentation",
  "readme": "README.md",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "categorizeByGroup": true,
  "categoryOrder": ["Core", "Client", "Server", "Solana", "Relayer"],
  "navigationLinks": {
    "GitHub": "https://github.com/example/px402",
    "Demo": "http://localhost:3404"
  },
  "plugin": ["typedoc-plugin-markdown"],
  "outputFileStrategy": "modules",
  "entryFileName": "index",
  "mergeReadme": true,
  "exclude": [
    "**/node_modules/**",
    "**/__tests__/**",
    "**/examples/**",
    "**/demo/**"
  ],
  "skipErrorChecking": true
}
