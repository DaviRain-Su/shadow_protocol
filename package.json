{
  "name": "px402",
  "version": "0.1.0",
  "private": true,
  "description": "Private x402 Protocol - Agent-to-Agent privacy payment",
  "repository": {
    "type": "git",
    "url": "https://github.com/DaviRain-Su/shadow_protocol.git"
  },
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "eslint packages/*/src --ext .ts",
    "format": "prettier --write 'packages/*/src/**/*.ts'",
    "clean": "pnpm -r clean",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "demo": "pnpm --filter @px402/demo start"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.2.0",
    "typedoc": "^0.28.16",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.0.0"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "packageManager": "pnpm@9.15.0"
}
